<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.ssh.domain.Project" table="project" schema="pms">
        <id name="projectId" column="projectId"/>
        <property name="projectName" column="projectName"/>
        <property name="projectType" column="projectType"/>
        <property name="leader" column="leader"/>
        <property name="hostGroup" column="hostGroup"/>
        <property name="projectManager" column="projectManager"/>
        <property name="efficiency" column="efficiency"/>
        <property name="functionPoint" column="functionPoint"/>
        <property name="workload" column="workload"/>
        <property name="startTime" column="startTime"/>
        <property name="endTime" column="endTime"/>
        <property name="isDelete" column="isDelete"/>
    </class>
</hibernate-mapping>