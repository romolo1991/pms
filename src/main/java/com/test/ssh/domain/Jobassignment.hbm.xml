<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.ssh.domain.Jobassignment" table="jobassignment" schema="pms">
        <id name="assignmentId" column="assignmentId"/>
        <property name="project" column="project"/>
        <property name="workload" column="workload"/>
        <property name="monthOfAssignment" column="monthOfAssignment"/>
        <property name="employee" column="employee"/>
        <property name="isDelete" column="isDelete"/>
    </class>
</hibernate-mapping>