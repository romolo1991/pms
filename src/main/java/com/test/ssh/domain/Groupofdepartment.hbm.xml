<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.ssh.domain.Groupofdepartment" table="groupofdepartment" schema="pms">
        <id name="groupId" column="groupId"/>
        <property name="groupName" column="groupName"/>
        <property name="introduction" column="introduction"/>
        <property name="department" column="department"/>
        <property name="groupLeader" column="groupLeader"/>
        <property name="isDelete" column="isDelete"/>
    </class>
</hibernate-mapping>