<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.ssh.domain.Employee" table="employee" schema="pms">
        <id name="employeeId" column="employeeId"/>
        <property name="employeeName" column="employeeName"/>
        <property name="employeeNameSpell" column="employeeNameSpell"/>
        <property name="entryTime" column="entryTime"/>
        <property name="department" column="department"/>
        <property name="groupOfEmployee" column="groupOfEmployee"/>
        <property name="groupLeader" column="groupLeader"/>
        <property name="isDelete" column="isDelete"/>
        <property name="remark1" column="remark1"/>
        <property name="remark2" column="remark2"/>
    </class>
</hibernate-mapping>